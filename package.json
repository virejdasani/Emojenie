{
    "name": "geniemoji",
    "version": "4.0.0",
    "description": "The Emoji Genie App",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "pack-darwin-x64": "electron-packager . Geniemoji --platform=darwin --arch=x64 --icon=assets/AppIcons/icon.icns --ignore=builds --ignore=docs --ignore=assets/PreviewImg --ignore=assets/PreviewVid --ignore=assets/oldIcons",
        "pack-win32-x64": "electron-packager . Geniemoji --platform=win32 --arch=x64 --icon=assets/AppIcons/icon.ico --ignore=builds --ignore=docs --ignore=assets/PreviewImg --ignore=assets/PreviewVid --ignore=assets/oldIcons",
        "pack-linux-x64": "electron-packager . Geniemoji --platform=linux --arch=x64 --icon=assets/AppIcons/icon.png --ignore=builds --ignore=docs --ignore=assets/PreviewImg --ignore=assets/PreviewVid --ignore=assets/oldIcons"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/virejdasani/Emojenie.git"
    },
    "keywords": [
        "Geniemoji",
        "Emoji",
        "Genie",
        "Emogenie",
        "Emojenie"
    ],
    "author": "Virej Dasani",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/virejdasani/Emojenie/issues"
    },
    "homepage": "https://github.com/virejdasani/Emojenie#readme",
    "dependencies": {
        "electron": "^12.0.4",
        "open": "^8.0.9",
        "robotjs": "^0.6.0"
    }
}
